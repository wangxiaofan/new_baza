<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/tv_change_type_title"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginTop="@dimen/dp_5"
        android:paddingBottom="@dimen/dp_5"
        android:text="@string/update_item_previous"
        android:textColor="@color/text_color_grey_94A1A5"
        android:textSize="@dimen/text_size_10"
        android:visibility="gone"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@id/view_resume_ele_hunter_job"
        android:layout_width="@dimen/dp_30"
        android:layout_height="@dimen/dp_30"
        android:src="@drawable/icon_inf_find_job"
        android:visibility="gone"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@id/view_resume_ele_real_name"
        style="@style/style_resume_ele_real_name"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginTop="@dimen/dp_15"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_change_type_title" />

    <TextView
        android:id="@id/view_resume_ele_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_5"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:maxLines="1"
        android:textColor="@color/text_color_black_4E5968"
        android:textSize="@dimen/text_size_12"
        app:layout_constraintBottom_toBottomOf="@id/view_resume_ele_real_name"
        app:layout_constraintLeft_toRightOf="@id/view_resume_ele_real_name"
        app:layout_constraintTop_toTopOf="@id/view_resume_ele_real_name" />

    <TextView
        android:id="@id/view_resume_ele_source_from"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_15"
        android:layout_marginLeft="@dimen/dp_5"
        android:layout_marginRight="@dimen/dp_15"
        android:background="@drawable/resume_text_bg_source_from"
        android:gravity="center"
        android:paddingLeft="@dimen/dp_5"
        android:paddingRight="@dimen/dp_5"
        android:textColor="@color/text_color_blue_53ABD5"
        android:textSize="@dimen/text_size_9"
        app:layout_constraintBottom_toBottomOf="@id/view_resume_ele_real_name"
        app:layout_constraintLeft_toRightOf="@id/view_resume_ele_title"
        app:layout_constraintTop_toTopOf="@id/view_resume_ele_real_name" />


    <TextView
        android:id="@id/view_resume_ele_second_main_info"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_marginRight="@dimen/dp_15"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:maxLines="1"
        android:textColor="@color/text_color_black_4E5968"
        android:textSize="@dimen/text_size_10"
        app:layout_constraintLeft_toLeftOf="@id/view_resume_ele_real_name"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view_resume_ele_real_name" />

    <TextView
        android:id="@id/view_resume_ele_hukou_info"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_marginRight="@dimen/dp_15"
        android:ellipsize="end"
        android:maxLines="1"
        android:textColor="@color/text_color_black_4E5968"
        android:textSize="@dimen/text_size_10"
        app:layout_constraintLeft_toLeftOf="@id/view_resume_ele_real_name"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view_resume_ele_second_main_info" />

    <TextView
        android:id="@id/view_resume_ele_company_info"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_marginRight="@dimen/dp_15"
        android:drawableLeft="@drawable/icon_company"
        android:drawablePadding="@dimen/dp_5"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:maxLines="1"
        android:textColor="@color/text_color_black_4E5968"
        android:textSize="@dimen/text_size_10"
        app:layout_constraintLeft_toLeftOf="@id/view_resume_ele_real_name"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view_resume_ele_hukou_info" />

    <TextView
        android:id="@id/view_resume_ele_school_info"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_marginRight="@dimen/dp_15"
        android:drawableLeft="@drawable/icon_school"
        android:drawablePadding="@dimen/dp_5"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:maxLines="1"
        android:textColor="@color/text_color_black_4E5968"
        android:textSize="@dimen/text_size_10"
        app:layout_constraintLeft_toLeftOf="@id/view_resume_ele_real_name"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view_resume_ele_company_info" />

    <TextView
        android:id="@id/view_resume_ele_sub_info"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_marginRight="@dimen/dp_15"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:maxLines="1"
        android:textColor="@color/text_color_black_4E5968"
        android:textSize="@dimen/text_size_10"
        app:layout_constraintLeft_toLeftOf="@id/view_resume_ele_real_name"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view_resume_ele_school_info" />

    <com.baza.android.bzw.widget.LineBreakLayout
        android:id="@id/view_resume_ele_labels"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_marginRight="@dimen/dp_15"
        app:horizontalDepart="@dimen/dp_5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view_resume_ele_sub_info"
        app:verticalDepart="@dimen/dp_5" />

    <TextView
        android:id="@+id/tv_new_tag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_8"
        android:background="@drawable/updated_content_tag_bg"
        android:paddingLeft="@dimen/dp_5"
        android:paddingRight="@dimen/dp_5"
        android:text="@string/update"
        android:textColor="@android:color/white"
        android:textSize="@dimen/text_size_10"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@id/view_resume_ele_real_name"
        app:layout_constraintLeft_toRightOf="@id/view_resume_ele_source_from"
        app:layout_constraintTop_toTopOf="@id/view_resume_ele_real_name" />
</androidx.constraintlayout.widget.ConstraintLayout>
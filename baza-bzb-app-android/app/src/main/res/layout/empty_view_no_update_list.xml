<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:orientation="vertical"
        android:padding="30dp">
        <TextView
            android:id="@+id/tv_hint_no_result"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawablePadding="15dp"
            android:drawableTop="@drawable/image_no_search_result"
            android:gravity="center"
            android:text="@string/search_result_no_value"
            android:textColor="@color/text_color_grey_9E9E9E"
            android:textSize="@dimen/text_size_13" />

        <TextView
            android:id="@+id/tv_import_resume"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="10dp"
            android:drawablePadding="5dp"
            android:gravity="center"
            android:text="@string/import_now_b"
            android:textColor="@drawable/text_color_selector_orange_gray" />
    </LinearLayout>

</FrameLayout>
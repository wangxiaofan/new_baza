<resources>

    <!-- Base application theme. -->
    <!--<style name="AppTheme" parent="Base.Theme.AppCompat.Light">-->
    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
        <item name="android:windowAnimationStyle">@style/activityAnimation</item>
        <item name="android:windowBackground">@android:color/white</item>
        <item name="android:windowNoTitle">true</item>
        <!--<item name="android:statusBarColor">@color/status_bar_default_color</item>-->
        <!--<item name="floatingActionButtonStyle">@style/AppTheme.Fab</item>-->
        <!--禁止多点触控-->
        <item name="android:windowEnableSplitTouch">false</item>
        <item name="android:splitMotionEvents">false</item>
        <item name="android:navigationBarColor">@color/background_main</item>
        <item name="android:colorAccent">@color/colorAccent</item>
        <item name="android:windowContentTransitions">true</item>
    </style>

    <style name="LargeeImagTheme" parent="AppTheme">
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:windowTranslucentNavigation">true</item>
    </style>

</resources>
